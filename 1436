import java.util.*;
import java.io.*;
public class Main {
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		PrintStream sa = System.out;
		int t = sc.nextInt();
		for(int i=0;i<t;i++){
			int n = sc.nextInt();
			for(int j=1;j<=n;j++){
				int x = sc.nextInt();
				if(j == n/2+1){
					sa.println("Case "+(i+1)+": "+x);
				}
			}
		}

	}
 }
