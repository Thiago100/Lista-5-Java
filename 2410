import java.util.*;
import java.io.*;
public class Main {
    public static long[] vetor=new long[66];
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
        int a[] = new int[1000005],x,soma = 0,n = sc.nextInt(),maior = 0;
        for(int i=0;i<n;i++){
                x = sc.nextInt();
                a[x]++;
                if(a[x]==1) soma++;
        }
        sa.println(soma);

    }
}
