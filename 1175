import java.util.*;
import java.io.*;
public class Main{
	public static void main(String[] args){
        PrintStream sa = System.out;
        Scanner sc = new Scanner(System.in);
        int v[],j=19;
        v = new int[20];
        for(int i=0;i<20;i++){
            v[i] = sc.nextInt();
        }
        for(int i=0;i<20;i++,j--){
            sa.println("N["+i+"] = "+v[j]);
        }

	}
}
